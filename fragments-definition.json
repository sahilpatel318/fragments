{
  "family": "fragments-task",
  "taskRoleArn": "arn:aws:iam::590183681901:role/LabRole",
  "executionRoleArn": "arn:aws:iam::590183681901:role/LabRole",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "fragments-container",
      "image": "590183681901.dkr.ecr.us-east-1.amazonaws.com/fragments:latest",
      "cpu": 256,
      "memoryReservation": 512,
      "portMappings": [
        {
          "name": "fragments-container-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "PORT",
          "value": "80"
        },
        {
          "name": "LOG_LEVEL",
          "value": "debug"
        },
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "AWS_REGION",
          "value": "us-east-1"
        },
        {
          "name": "AWS_S3_BUCKET_NAME",
          "value": "avinav-fragments"
        },{
          "name": "AWS_DYNAMODB_TABLE_NAME",
          "value": "fragments"
        }
      ],
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "systemControls": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/fragments-task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "placementConstraints": [],
  "volumes": []
 }